.react-ui-slider {
    height: 30px;
    width: 100%;
}

.react-ui-slider-track {
    background-color: lighten(#cdcdcd, 60%);
    border: 1px solid #cdcdcd;
    border-radius: 20px;
    height: 10px;
    top: 10px;
    width: 100%;
}

.react-ui-slider-fill {
    background-color: #66aacc;
    border-radius: 20px;
    height: 10px;
    top: 0;
    left: 0;
    width: 0;
}

.react-ui-slider-handle {
    background-color: darken(#cdcdcd, 20%);
    border: 1px solid #cdcdcd;
    border-radius: 50%;
    height: 20px;
    left: 0;
    top: -6px;
    width: 20px;
}
